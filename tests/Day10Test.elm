module Day10Test exposing (..)

import Day10
import Expect
import Test exposing (..)


suite : Test
suite =
    describe "day 10"
        [ test "part 1 example" <|
            \() ->
                exampleInput
                    |> Day10.parseInput
                    |> Maybe.map Day10.solvePart1
                    |> Expect.equal (Just 26397)
        , test "part 1 answer" <|
            \() ->
                puzzleInput
                    |> Day10.parseInput
                    |> Maybe.map Day10.solvePart1
                    |> Expect.equal (Just 411471)

